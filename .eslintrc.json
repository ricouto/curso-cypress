{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "no-console":"off"
    },
    "linterOptions": { 
        "exclude": [
            "cypress/integration/**/*.js"
        ]
    }//fonte https://stackoverflow.com/questions/48884789/how-to-exclude-all-files-under-a-directory-in-lint-using-angular-cli
}
